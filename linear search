#include <stdio.h>
int main() {
    int a[5] = {2, 4, 6, 8, 10}, i, x, found = 0;
    printf("Enter element to search: ");
    scanf("%d", &x);
    for (i = 0; i < 5; i++)
        if (a[i] == x) { found = 1; break; }
    if (found) printf("Element found at position %d", i + 1);
    else printf("Element not found");
    return 0;
}
